    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}"
        },
        {
            "name": "Server",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server",
            "env": {
                "APPLICATION_ENVIRONMENT":"Development",
                "MODE":"ddd",
                "EXAMPLE__PORT": "1111",
                //"OIDC_CONFIG__AUTHORITY": "https://some.oidc.authority/",
                "ENABLE_TRANSIENT_2": "false"               
            },
            "args": ["serve", "-e=EXAMPLE__PORT=5105" ]
        },
    ]
}